# Sanity Check 3 for get_college_checklist
import pandas as pd
# Use previous matched colleges
if len(matched) > 0:
    for college in matched:
        checklist = get_college_checklist(college)
        assert isinstance(checklist, list)
        assert "Reviewed" in checklist
        print(f"✅ get_college_checklist works for {college['name']}! Checklist:", checklist) # Make checklist more legible
        print(f"\n{college['name']} Requirements:")
        req_df = pd.DataFrame({"Requirement": get_college_checklist(college)})
        display(req_df)
else:
    print("❌ No college matched for checklist test!")
